<p id="notice"><%= notice %></p>

<h1>Listing Download Lists</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @download_lists.each do |download_list| %>
      <tr>
        <td><%= download_list.first_name %></td>
        <td><%= download_list.last_name %></td>
        <td><%= download_list.email %></td>
        <td><%= link_to 'Show', download_list %></td>
        <td><%= link_to 'Edit', edit_download_list_path(download_list) %></td>
        <td><%= link_to 'Destroy', download_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Download list', new_download_list_path %>
